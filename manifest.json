{
    "manifest_version": 3,
    "name": "Bakaláři Predictor",
    "description": "Předvídač pro Bakaláře",
    "version": "0.0.1",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "action": {
        "default_title": "Bakaláři Predictor"
    },
    "permissions": [],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [{
        "js": ["foreground.js"],
        "matches": ["<all_urls>"],
        "run_at": "document_start"
    }]
}
